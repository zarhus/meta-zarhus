From 9bea372206356c7e65c6e57fc14f0409cfb9ebc7 Mon Sep 17 00:00:00 2001
From: Daniil Klimuk <daniil.klimuk@3mdeb.com>
Date: Wed, 4 Sep 2024 16:23:10 +0200
Subject: [PATCH] rk356x.dtsi: reserve optee memory

Signed-off-by: Daniil Klimuk <daniil.klimuk@3mdeb.com>
---
 arch/arm64/boot/dts/rockchip/rk356x.dtsi | 11 +++++++++++
 1 file changed, 11 insertions(+)

diff --git a/arch/arm64/boot/dts/rockchip/rk356x.dtsi b/arch/arm64/boot/dts/rockchip/rk356x.dtsi
index 2f885bc3665b..fff3cf8bb450 100644
--- a/arch/arm64/boot/dts/rockchip/rk356x.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk356x.dtsi
@@ -45,6 +45,17 @@ aliases {
 		spi3 = &spi3;
 	};
 
+	reserved-memory {
+		#address-cells = <2>;
+		#size-cells = <2>;
+		ranges;
+
+		optee@8400000 {
+			reg = <0x0 0x08400000 0x0 0x01000000>;
+                       no-map;
+		};
+	};
+
 	cpus {
 		#address-cells = <2>;
 		#size-cells = <0>;
-- 
